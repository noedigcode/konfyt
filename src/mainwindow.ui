QMainWindow {
	background-color: rgb(59, 59, 59);
	margin-top: 0px;
	padding-top: 0px;
}

/* ---------------------------------------------------------------------- */

/* Push Buttons */

QPushButton {
	border-top: 1px solid rgb(100,100,100);
    border-left: 1px solid rgb(100,100,100);
    border-right: 1px solid rgb(90,90,90);
    border-bottom: 1px solid rgb(75,75,75);
	
	background-color: qlineargradient(spread:pad, x1:0, y1:1, x2:0, y2:0, stop:0 rgba(75, 75, 75, 255), stop:1 rgba(85,85,85, 255));

	border-radius: 2px;
	padding: 4px;
}

QPushButton[KonfytWideButton=true] {
	min-width: 4em;
}

QPushButton:hover{
	border-top: 1px solid rgb(100,100,100);
    border-left: 1px solid rgb(100,100,100);
    border-right: 1px solid rgb(90,90,90);
    border-bottom: 1px solid rgb(75,75,75);
	
	background-color: qlineargradient(spread:pad, x1:0, y1:1, x2:0, y2:0, stop:0 rgba(85,85,85, 255), stop:1 rgba(100,100,100, 255));

	border-radius: 2px;
	padding: 4px;
}

QPushButton:pressed{
	border-top: 1px solid rgb(100,100,100);
    border-left: 1px solid rgb(100,100,100);
    border-right: 1px solid rgb(90,90,90);
    border-bottom: 1px solid rgb(75,75,75);
	
	background-color: qlineargradient(spread:pad, x1:0, y1:1, x2:0, y2:0, stop:0 rgba(50,50,50, 255), stop:1 rgba(65,65,65, 255));

	border-radius: 2px;
}

QPushButton::checked{
	color: red;
	background-color: rgb(54, 54, 54);
}

/* ---------------------------------------------------------------------- */
/* Sliders */

QSlider{
	background: rgba(0,0,0,0);
}

QSlider::groove:horizontal {
     border: 1px solid #999999;
     height: 2px; /* the groove expands to the size of the slider by default. by giving it a height, it has a fixed size */
     background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 rgba(75,75,75, 255), stop:1 rgba(59,59,59, 255));
     margin: 2px 0;
 }

QSlider::handle:horizontal {
     background-color: qlineargradient(x1:0, y1:1, x2:1, y2:0, stop:0 rgba(45, 62, 109, 255), stop:1 rgba(65, 97, 145, 210));
     border: 1px solid #5c5c5c;
     width: 18px;
     margin: -5px 0 -5px 0; /* handle is placed by default on the contents rect of the groove. Expand outside the groove */
	border-radius: 2px;
	border: 1px solid grey;
 }

QSlider::handle:horizontal:hover {
     background-color: qlineargradient(x1:0, y1:1, x2:1, y2:0, stop:0 rgba(45, 62, 109, 255), stop:1 rgba(65, 97, 145, 255));
     border: 1px solid #5c5c5c;
     width: 18px;
     margin: -5px 0 -5px 0; /* handle is placed by default on the contents rect of the groove. Expand outside the groove */
	border-radius: 2px;
	border: 1px solid grey;
 }

/* ---------------------------------------------------------------------- */

QSpinBox {
    border: 1px solid grey;
    border-radius: 2px;
}

QSpinBox:!enabled {
	background-color: rgb(77, 77, 77);
	color: grey;
}

QSpinBox::up-button {
	background-color: rgb(77, 77, 77);
     subcontrol-origin: border;
     subcontrol-position: top right; /* position at the top right corner */

    width: 16px; /* 16 + 2*1px border-width = 15px padding + 3px parent border */
    border: 1px solid transparent; /*rgb(77, 77, 77);*/
	margin-top: 1px;
	margin-right: 1px;
	
	image: url(:/icons/up.png);
 }

QSpinBox::down-button {
  	background-color: rgb(77, 77, 77);
     subcontrol-origin: border;
     subcontrol-position: bottom right; /* position at bottom right corner */

     width: 16px;
     border: 1px solid transparent;
	margin-bottom: 1px;
	margin-right: 1px;

	
	image: url(:/icons/down.png);
 }

 /* ---------------------------------------------------------------------- */

QSplitter::handle {
   background: transparent;
}

/* ---------------------------------------------------------------------- */
/* Scroll bars */

QScrollBar:vertical {
      border: 0px transparent;
      background: rgb(75,75,75);
      width: 14px;
      margin: 0 0 0 0;
	  /*padding: 14px 0 14px 0;*/
	padding: 0 0 0 0;
}
QScrollBar:horizontal {
      border: 0px transparent;
      background: rgb(75,75,75);
      height: 14px;
      margin: 0 0 0 0;
	  /*padding: 0px 14px 0px 14px;*/
	padding: 0 0 0 0;
}
QScrollBar::handle:vertical {
	background: rgb(120,120,120);
	min-height: 20px;
}
QScrollBar::handle:hover:vertical {
	background: rgb(160,160,160);
	min-height: 20px;
}
QScrollBar::handle:horizontal {
	background: rgb(120,120,120);
	min-width: 20px;
}
QScrollBar::handle:hover:horizontal {
	background: rgb(160,160,160);
	min-width: 20px;
}
/* Top scrollbar button */
QScrollBar::sub-line:vertical {
	background: rgb(100,100,100);
    /*height: 14px;*/
	height: 0;
    subcontrol-position: top;
    subcontrol-origin: padding;
	
}
/* Bottom scrollbar button */
QScrollBar::add-line:vertical {
      background: rgb(100,100,100);
      /*height: 14px;*/
	 height: 0;
      subcontrol-position: bottom;
      subcontrol-origin: padding;
}
/* Left scrollbar button */
QScrollBar::sub-line:horizontal {
      background: rgb(100,100,100);
      /*width: 14px;*/
	  width: 0;
      subcontrol-position: left;
      subcontrol-origin: padding;
}
/* Right scrollbar button */
QScrollBar::add-line:horizontal {
	background: rgb(100,100,100);
	/*width: 14px;*/
	width: 0;
	subcontrol-position: right;
	subcontrol-origin: padding;
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
	background: none;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
	background: none;
}


/* ---------------------------------------------------------------------- */
/* Tab widget */

QTabWidget::pane { /* The tab widget frame */
     border-top: 0px solid grey;
	
 }

 QTabWidget::tab-bar {
     left: 5px; /* move to the right by 5px */

 }

 /* Style the tab using the tab sub-control. Note that
     it reads QTabBar _not_ QTabWidget */
 QTabBar::tab {
     background: rgb(50,50,50);
     border: 1px solid rgb(100,100,100);
     border-top-left-radius: 2px;
     border-top-right-radius: 2px;
     min-width: 3ex;
     padding: 1px;
	height:20px;
 }

 QTabBar::tab:selected, QTabBar::tab:hover {
     background: rgb(100,100,100);
 }

 QTabBar::tab:selected {
     border-color: #9B9B9B;
	height: 19px;
 }

 QTabBar::tab:!selected {
     /*margin-top: 2px; /* make non-selected tabs look smaller */
	height: 18px;
	margin-top: 1px;
 }


/* ---------------------------------------------------------------------- */
QTextBrowser {
	background-color: rgb(50, 50, 50);
	border: 1px solid;
	border-color: rgb(77, 77, 77);
}

/* Patch note textbrowser */
QTextBrowser#textBrowser_patchNote {
	border: 1px solid grey;
	background-color: rgba(0,0,0,0);
}

/* ---------------------------------------------------------------------- */
QTreeView {
	background-color: rgb(50, 50, 50);
	border: 1px solid;
	border-color: rgb(77, 77, 77);
}

QTreeView::branch {
	border-image: none;
	image: none;
}

/* ---------------------------------------------------------------------- */
/* Tool Buttons */

QToolButton {
	border-top: 1px solid rgb(100,100,100);
    border-left: 1px solid rgb(100,100,100);
    border-right: 1px solid rgb(90,90,90);
    border-bottom: 1px solid rgb(75,75,75);
	
	background-color: qlineargradient(spread:pad, x1:0, y1:1, x2:0, y2:0, stop:0 rgba(75, 75, 75, 255), stop:1 rgba(85,85,85, 255));

	border-radius: 2px;
	padding: 0px;
    height: 23px; /* TODO */

}

QToolButton:hover {
	border-top: 1px solid rgb(100,100,100);
    border-left: 1px solid rgb(100,100,100);
    border-right: 1px solid rgb(90,90,90);
    border-bottom: 1px solid rgb(75,75,75);
	
	background-color: qlineargradient(spread:pad, x1:0, y1:1, x2:0, y2:0, stop:0 rgba(85,85,85, 255), stop:1 rgba(100,100,100, 255));

	border-radius: 2px;
	padding: 0px;
}

QToolButton::pressed{
	border-top: 1px solid rgb(100,100,100);
    border-left: 1px solid rgb(100,100,100);
    border-right: 1px solid rgb(90,90,90);
    border-bottom: 1px solid rgb(75,75,75);
	
	background-color: qlineargradient(spread:pad, x1:0, y1:1, x2:0, y2:0, stop:0 rgba(50,50,50, 255), stop:1 rgba(65,65,65, 255));

	border-radius: 2px;
}

QToolButton::checked{
	color: red;
	background-color: rgb(54, 54, 54);
}

QToolButton[popupMode=&quot;1&quot;] { /* only for MenuButtonPopup */
     padding-right: 17px; /* make way for the popup button */
 }

 /* The subcontrols below are used only in the MenuButtonPopup mode */
 QToolButton::menu-button {
     border: 0px solid gray;
     border-left: 1px solid gray;
	border-top-right-radius: 2px;
	border-top-left-radius: 0px;
	border-bottom-right-radius: 2px;
	border-bottom-left-radius: 0px;
	background-color: rgba(75, 75, 75, 255);
     /* 16px width + 4px for border = 20px allocated above */
     width: 16px;
 }

 QToolButton::menu-button:hover {
     background-color: qlineargradient(spread:pad, x1:0, y1:1, x2:0, y2:0, stop:0 rgba(85,85,85, 255), stop:1 rgba(100,100,100, 255));
 }


 QToolButton::menu-arrow {
	border-image: url(:/icons/down.png);
 }


 QToolButton::menu-arrow:open {
     top: 1px; left: 1px; /* shift it a bit */
 }

QToolButton::menu-indicator {
	width: 0;
}

/* ---------------------------------------------------------------------- */

QToolTip {
	font: 9pt;
	background-color: white;
	color: black;

}

/* ---------------------------------------------------------------------- */

QStatusBar {
    background-color: rgb(80, 80, 80);
}

