/* ---------------------------------------------------------------------- */
QCheckBox::indicator {
	border: 1px solid grey;
    width: 13px;
    height: 13px;
}

QCheckBox::indicator:checked {
	background-color: rgb(19, 236, 47);
}

/* ---------------------------------------------------------------------- */
/* MIDI Indicator */
QCheckBox#MIDI_indicator::indicator {
	border: 1px solid grey;
	border-radius: 4px;
}

QCheckBox#MIDI_indicator::indicator:checked {
	background-color: rgb(19, 236, 47);
}

QCheckBox#MIDI_indicator_sustain::indicator {
	border: 1px solid grey;
	border-radius: 4px;
}

QCheckBox#MIDI_indicator_sustain::indicator:checked {
	background-color: rgb(19, 236, 47);
}

QCheckBox#MIDI_indicator_pitchbend::indicator {
	border: 1px solid grey;
	border-radius: 4px;
}

QCheckBox#MIDI_indicator_pitchbend::indicator:checked {
	background-color: rgb(19, 236, 47);
}

/* ---------------------------------------------------------------------- */
/* ComboBox */

QComboBox {
	background-color: rgb(54, 54, 54);
	border: 1px solid grey;
	border-radius: 2px;
	color: rgb(245,245,245);
	padding: 2px; /* Padding required to make color work. */
}

QComboBox::drop-down {
     subcontrol-origin: padding;
     subcontrol-position: top right;
     width: 16px;
	 background-color: rgb(70,70,70);
     border-left-width: 1px;
     border-left-color: rgb(30,30,30);
     border-left-style: solid; /* just a single line */
     border-top-right-radius: 3px; /* same radius as the QComboBox */
     border-bottom-right-radius: 3px;
	
	image: url(:/icons/down.png);
 }

QComboBox QAbstractItemView {
     border: 2px solid darkgray;
     selection-background-color: qlineargradient(spread:pad, x1:0, y1:1, x2:1, y2:0, stop:0 rgba(45, 62, 109, 255), stop:1 rgba(65, 97, 145, 255));
}

/* ---------------------------------------------------------------------- */
/* Frames that hold buttons */
QFrame[buttonFrame=true] {
	background: rgb(40,40,40);
    padding: 1px;
	border-radius: 3px;
	min-height: 1.5em;
	max-height: 1.5em;
}

/* ---------------------------------------------------------------------- */
/* Main toolbar frame*/
QFrame#frame_MainToolbar{
	margin-top: 0px;
	border: 0px;
	border-radius: 0px;
	background-color: rgb(80, 80, 80);
}

/* ---------------------------------------------------------------------- */
/* GroupBox */

QGroupBox{
	margin-top: 2ex;	
	background-color:  rgba(107, 107, 107, 255);
	border: 2px solid rgb(212, 212, 212);
	border-radius: 1px;
	padding: 1px;
}

QGroupBox::title {
	subcontrol-position: top left;
	subcontrol-origin: margin;
	padding: 0 3px;
	padding-top: 2px;
}

/* ---------------------------------------------------------------------- */
/* Patch page groupbox */
QGroupBox#groupBox_PatchPage {	
	margin-top: 0ex;

	background-color: rgb(4, 59, 93);
	
	background-color: qlineargradient(spread:pad, x1:0, y1:1, x2:0.403, y2:0.0965909, stop:0 rgba(2, 33, 56, 255), stop:1 rgba(0, 0, 0, 255));
	
	border-top-color: qlineargradient(spread:pad, x1:1, y1:0, x2:0, y2:0, stop:0 rgba(70, 70, 70, 255), stop:1 rgba(59, 59, 59, 255));
	border-left-color: qlineargradient(spread:pad, x1:0, y1:1, x2:0, y2:0, stop:0 rgba(70, 70, 70, 255), stop:1 rgba(59, 59, 59, 255));
	border-right-color: qlineargradient(spread:pad, x1:0, y1:1, x2:0, y2:0, stop:0 rgba(120, 120, 120, 255), stop:1 rgba(70, 70, 70, 255));
	border-bottom-color: qlineargradient(spread:pad, x1:1, y1:0, x2:0, y2:0, stop:0 rgba(120, 120, 120, 255), stop:1 rgba(70, 70, 70, 255));
